#!ipxe

goto ${menu} ||

:live_menu
set os Live Raizo
menu ${os}
item --gap ${os} Versions
item current ${space} ${os} current
choose live_version || goto live_exit
goto ${live_version}


:current
set squash_url ${live_endpoint}/debian-squash/releases/download/v12.21.09.05i-0aa6f3c9/filesystem.squashfs
set kernel_url ${live_endpoint}/debian-squash/releases/download/v12.21.09.05i-0aa6f3c9/
goto boot


:boot
imgfree
kernel ${kernel_url}vmlinuz boot=live fetch=${squash_url} initrd=initrd.magic ${cmdline}
initrd ${kernel_url}initrd
boot

:live_exit
clear menu
exit 0
